#include<stdio.h>
#include<windows.h>
#include<time.h>

int main() {
			int cnt = 0;

				while (1)
				{
					printf("RED\n");
					Sleep(1000);
					printf("BLUE\n");
					Sleep(1000);
					printf("YELLOW\n");
					Sleep(1000);
					cnt++;

						if (cnt == 10)
						{
							return 0;
						}
				}
			}
	
