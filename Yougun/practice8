int main(void) {
	int cnt = 1;
	//char c;

	while (cnt) {
		getchar();
		//while (getchar() != '\n');
		printf("RED");
		cnt++;
		getchar();
		//while (getchar() != '\n');
		printf("BLUE");
		cnt++;
		getchar();
		//while (getchar() != '\n');
		printf("YELLOW");
		cnt++;
		
		if (cnt > 21) {
			break;
			}
		}


	return 0;
	}
