int main(void) {
	int cnt = 0;
	int ch;
	while (1)
		{
		ch = getchar();
		printf("RED");
		break;
		}
	while (1)
	{
		ch = getchar();
		printf("BLUE");
		break;
	}
	while (1)
	{
		ch = getchar();
		printf("YELLOW");
		break;
	}
	while (1)
	{
		ch = getchar();
		printf("RED");
		break;
	}
	while (1)
	{
		ch = getchar();
		printf("BLUE");
		break;
	}
	while (1)
	{
		ch = getchar();
		printf("YELLOW");
		break;
	}
}

	
